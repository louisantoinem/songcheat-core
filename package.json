{
  "name": "songcheat-core",
  "version": "1.0.0",
  "description": "Base classes for handling SongCheat files. Can be used both in Node and in browser.",
  "main": "dist/songcheat-core.cjs.js",
  "module": "dist/songcheat-core.esm.js",
  "watch": {
    "template": {
      "patterns": [
        "template"
      ],
      "extensions": "txt",
      "quiet": true
    }
  },
  "scripts": {
    "clean": "rm -f dist/*",
    "lint": "standard --fix src/*js test/*.js *.js",
    "start": "npm run lint && node template/convert && rm dist/songcheat-core.esm.js && ln -s ../src/main.js dist/songcheat-core.esm.js && rollup -cw",
    "test": "node test",
    "build": "npm run clean && npm run lint && node template/convert && rollup -c rollup.config.build.js",
    "template": "node template/convert.js",
    "watch": "npm-watch"
  },
  "keywords": [
    "songcheat",
    "music",
    "song",
    "scoresheet",
    "notation",
    "chords"
  ],
  "author": "Louis Antoine-Moussiaux",
  "license": "ISC",
  "dependencies": {
    "@mohayonao/wave-tables": "git+https://github.com/mohayonao/wave-tables.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "npm-watch": "^0.3.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  }
}
